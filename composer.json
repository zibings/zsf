{
	"name": "zibings/zsf",
	"description": "Zibings Site Framework",
	"type": "project",
	"license": "MIT",
	"minimum-stability": "dev",
	"repositories": [
		{
			"url": "https://github.com/AndyM84/pseudo.git",
			"type": "vcs"
		}
	],
	"autoload" : {
		"files": [
			"scripts/setup.php"
		]
	},
	"require" : {
		"php" : "^8.3",
		"stoic/web" : "^1.3",
		"phpmailer/phpmailer" : "^6.2",
		"hybridauth/hybridauth" : "*",
		"ext-curl" : "*",
		"ext-pdo" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.5",
		"zircote/swagger-php" : "^4",
		"psr/cache" : "^3",
		"doctrine/annotations" : "^1",
		"psr/simple-cache" : "^3",
		"symfony/browser-kit" : "^6",
		"symfony/dependency-injection" : "^6",
		"symfony/event-dispatcher" : "^6",
		"symfony/config" : "^6",
		"symfony/expression-language" : "^6",
		"symfony/http-kernel" : "^6",
		"symfony/messenger" : "^6",
		"symfony/console" : "^6",
		"symfony/serializer" : "^6",
		"symfony/translation-contracts" : "^3",
		"jimbojsb/pseudo": "dev-AndyM84-8_3"
	},
	"scripts" : {
		"post-install-cmd" : [
			"@php -r \"echo 'Current directory: ' . getcwd() . PHP_EOL;\"",
			"@php -f scripts/setup.php > setup-output.log 2>&1"
		],
		"post-create-project-cmd" : [
			"@php -f scripts/setup.php"
		]
	}
}
