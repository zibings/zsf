{
	"name": "front",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"preview": "vite preview",
		"build": "run-p type-check \"ssg {@}\" --",
		"build:d": "run-p type-check \"build:only {@}\" --",
		"build:only": "vite build",
		"ssg": "run-p \"ssg:build {@}\" --",
		"ssg:build": "cross-env VITE_ENV_MODE=ssg DEBUG=vite-ssg:* vite-ssg build",
		"ssg:staging": "pnpm ssg:build --mode staging",
		"staging": "cross-env VITE_ENV_MODE=ssg vite build --mode staging",
		"test:unit": "vitest",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"zsf": "cross-env APP_TARGET=../../web vite build"
	},
	"dependencies": {
		"@primevue/themes": "4.0.0",
		"axios": "^1.7.2",
		"chart.js": "^4.4.3",
		"pinia": "^2.1.7",
		"primevue": "4.0.0",
		"vite-ssg": "^0.23.8",
		"vue": "^3.4.33",
		"vue-router": "^4.4.0"
	},
	"devDependencies": {
		"@ctd1500/eslint-config": "^0.2.0",
		"@primevue/auto-import-resolver": "^4.0.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@tsconfig/node20": "^20.1.4",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^20.14.11",
		"@vitejs/plugin-vue": "^5.0.5",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.5.1",
		"critters": "^0.0.24",
		"cross-env": "^7.0.3",
		"eslint": "^9.7.0",
		"jsdom": "^24.1.0",
		"npm-run-all2": "^6.2.2",
		"prettier": "^3.3.3",
		"sass": "^1.77.8",
		"typescript": "~5.5.3",
		"unplugin-icons": "^0.19.0",
		"unplugin-vue-components": "^0.27.3",
		"vite": "^5.3.4",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.6.0",
		"vue-tsc": "^2.0.26"
	},
	"packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
